    <script type="text/javascript">
    function asset(src) {
        return 'http://trycatch.com.au/bundles/trycatchdashboard/' + src;
    };
        var dataSources = {

            'releaseDates'  : 'http://trycatch.com.au/api/channels/1/releasedates', // release dates for production
            'components'    : 'http://trycatch.com.au/api/components',
                /*
                    Get all of the current components (CAM, Fatwire) at a global level
                    http://trycatch.com.au/api/components/1 => Get a single component
                    http://trycatch.com.au/api/components/1/channels => Get a single components channels
               */
            'channels'      : 'http://trycatch.com.au/api/channels',
                /*
                    Get all of the current channels
                    http://trycatch.com.au/api/channels/1 => Get a single channel
                    http://trycatch.com.au/api/channels/1/components => Get a channels components
                    http://trycatch.com.au/api/channels/1/components/1 => Get the detailed information about a single channels single component
                 */
            'graphSummary' : 'http://trycatch.com.au/api/issues/graph'
        };

        if (useApi) {
            /* This will get the current highest priority SM7 call status for open tickets */
            document.write('<script type="text/javascript" src="http://trycatch.com.au/api/issues/highest.js?varname=highest"><' + '/script>');
                } else { // if we're not using the API we will get a local copy of the information we need. Should not be an issue when we're on the same system
            document.write('<script type="text/javascript" src="channels.js"><' + '/script>');
            document.write('<script type="text/javascript" src="components.js"><' + '/script>');
            document.write('<script type="text/javascript" src="graph.js"><' + '/script>');
                }
</script>
